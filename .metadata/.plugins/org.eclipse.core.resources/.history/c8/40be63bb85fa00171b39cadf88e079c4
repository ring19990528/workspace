package exam;

public class Bubble {

	public static void main(String[] args) {
		int n[] = { 45, 81, 9, 72, 4, 6};
		for (int i = 0;i < n.length - 1;i = i+1 ){
			for(int j = i + 1;j < n.length;j = j+1){
				if(n[j]>n[i]){
					int tmp = n[i];
					n[i] = n[j];
					n[j] = tmp; 
				}
			}
		}
		for(int num : n){
			System.out.print(num + " ");
		}
	}

}
